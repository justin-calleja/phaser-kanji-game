{
  "version": 1,
  "projects": {
    "webstore": {
      "root": "libs/webstore",
      "sourceRoot": "libs/webstore/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/webstore/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/webstore/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "kapp": {
      "root": "apps/kapp",
      "sourceRoot": "apps/kapp/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/kapp",
            "index": "apps/kapp/src/index.html",
            "main": "apps/kapp/src/main.tsx",
            "polyfills": "apps/kapp/src/polyfills.ts",
            "tsConfig": "apps/kapp/tsconfig.app.json",
            "assets": ["apps/kapp/src/favicon.ico", "apps/kapp/src/assets"],
            "styles": ["apps/kapp/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/kapp/src/environments/environment.ts",
                  "with": "apps/kapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "kapp:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "kapp:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/kapp/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/kapp/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "kapp-e2e": {
      "root": "apps/kapp-e2e",
      "sourceRoot": "apps/kapp-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/kapp-e2e/cypress.json",
            "tsConfig": "apps/kapp-e2e/tsconfig.e2e.json",
            "devServerTarget": "kapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "kapp:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/kapp-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "kapp"
}
